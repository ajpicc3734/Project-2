<h3>Records</h3>
<div id="forum">
<ol>
  {{#each records}}
  <li>
    <article class="post">
      <div class="title">
      
      </div>
      <div>Artist: {{artist}}</div>
      <div>Title: {{title}} </div>
      {{!-- <div>filename: {{filename}}</div> --}}
      <div>Belongs to {{user.username}} </div>
      <div><a href="mailto:{{email}}">Inquire About This Record</a></div>
      <div class="meta">

        {{!-- <a href="/post/{{id}}">{{comments.length}} comment(s)</a> --}}
      </div>
    </article>
  </li>
  {{/each}}
</ol>
</div>
