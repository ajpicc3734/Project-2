<h3>Users</h3>

<div id="forum">

<ol>
  {{#each users}}
  <li>
    <article class="post">
      <div class="title">
      
      </div>
      <div>Username: {{username}}</div>
      <div>Record Collection </div>
      {{#each records}}
      <li>{{title}}</li>
      {{/each}}
      <div><a href="mailto:{{email}}">Inquire About This Collection</a></div>
      <div class="meta">
      </div>
    </article>
  </li>
  {{/each}}
</ol>
</div>


